      "role": "Sr. FullStack Developer."
    }
  ],
  "require": {
    "php": ">=7.4 || ^8.0",
    "fideloper/proxy": "^4.0",
    "laravel/framework": "^8.0",
    "laravel/tinker": "^2.0",
    "elgibor-solution/laravel-database-encryption": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "LarashCuts\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LarashCuts\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php ./vendor/bin/testbench package:discover --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "LarashCuts\\LaraShCutsServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^6.19",
    "phpunit/phpunit": "^9.5"
  }
}